			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%d [%t] %-5p [%c] %m%n" />
			</layout>
		</appender>
		<root>
			<level value="WARN" />
		</root>
		<logger name="CrexiWeather">
			<level value="ALL" />
			<appender-ref ref="CrexiWeatherAppender" />
		</logger>
	</log4net>
